section .data
    a db 3
    b db 4

section .text
    global _start

_start:
    mov esi, [a]
    mov edi, [b]

    mov eax, esi
    mul edi
    add eax, eax
    cmp esi, edi
    jl if
    jmp else

if:
    mov esi, eax
    jmp print_result

else:
    mov edi, eax
    jmp print_result

print_result:
    mov ecx, esi
    mov ebx, 1
    mov edx, esi
    mov eax, 4
    int 0x80

    mov ecx, edi
    mov ebx, 1
    mov edx, edi
    mov eax, 4
    int 0x80

    mov eax, 1
    int 0x80
